class IfWhileTest {

    def void main() {
        int a;
        double b;
        char c;
        if (a > b) {
            while (a != 0) {
                a = a - 1;
                while (b != 0) {
                    b = b - 1;
                    if (b == 7) {
                        continue;
                    } else {
                        if (b <= a) {
                            break;
                        } else {
